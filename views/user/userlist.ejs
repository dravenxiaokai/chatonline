<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>用户列表</title>
    <%- include ../common/head%>
    <style>
        a:hover{text-decoration:none;}
    </style>
</head>
<body>
    <div class="container">
        <h2>用户列表</h2>
        <ul class="list-unstyled">
            <%users.forEach(function(item){%>
                <li class="text-center text-muted"><%=item.nickname%></li>
            <%})%>
        </ul>
        <ul class="list-unstyled">
            <li class="text-center text-muted"><a href="#">xiaozhang</a></li>
            <li class="text-center text-muted">xiaozhang</li>
            <li class="text-center text-muted">xiaozhang</li>
            <li class="text-center text-muted">xiaozhang</li>
            <li class="text-center text-muted">xiaozhang</li>
        </ul>
        <table class="table table-striped table-hover">
            <thead>
                <th>
                    <tr>
                        <td class="text-center text-info"><strong>好友列表</strong></td>
                    </tr>
                </th>
            </thead>
            <tbody>
                <%users.forEach(function(item){%>
                <tr>
                    <td class="text-center text-primary"><a href="#" class="" ><%=item.nickname%></a></td>
                </tr>    
                <%})%>
            </tbody>
        </table>
    </div>
    <%- include ../common/foot%>
</body>
</html>